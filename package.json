{
  "name": "electron-admin",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "typeScriptVersion": "3.9",
  "types": "./typings/index.d.ts",
  "scripts": {
    "lint:staged:tslint": "",
    "postlint:staged:tslint": "npm run tslint",
    "tslint": "node_modules/.bin/tsc --noEmit --pretty",
    "app": "node electron-cli-service app",
    "kill": "node electron-cli-service kill",
    "start": "cross-env NODE_ENV=development LANGUAGE=zh_CN node electron-cli-service start",
    "build": "cross-env NODE_ENV=production LANGUAGE=zh_CN node electron-cli-service build",
    "start:en": "cross-env NODE_ENV=development LANGUAGE=en node electron-cli-service start",
    "build:en": "cross-env NODE_ENV=production LANGUAGE=en node electron-cli-service build",
    "test:build:ci": "cross-env BUILD_NUMBER=1015 npm run build",
    "codec:swagger": "node electron-cli-service autoService"
  },
  "dependencies": {
    "@ant-design/icons": "~4.3.0",
    "antd": "^4.17.3",
    "axios": "~0.21.0",
    "babel-polyfill": "~6.26.0",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "3",
    "dayjs": "~1.9.6",
    "docx": "^5.4.1",
    "electron-updater": "~4.3.5",
    "history": "~4.10.0",
    "koa": "~2.12.0",
    "koa-bodyparser": "~4.3.0",
    "koa-favicon": "~2.1.0",
    "koa-router": "~10.0.0",
    "koa-static": "~5.0.0",
    "mobx": "~5.15.7",
    "mobx-react": "~6.3.1",
    "mobx-react-router": "~4.1.0",
    "mousetrap": "^1.6.5",
    "nedb": "~1.8.0",
    "pubsub-js": "^1.9.3",
    "react": "~17.0.0",
    "react-dom": "~17.0.0",
    "react-router": "~5.2.0",
    "react-router-dom": "~5.2.0",
    "rsuite": "^5.2.4",
    "yarn": "^1.22.17"
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.10.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.9.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.3.3",
    "@types/koa": "~2.11.6",
    "@types/koa-bodyparser": "~4.3.0",
    "@types/koa-router": "~7.4.1",
    "@types/mousetrap": "^1.6.8",
    "@types/nedb": "~1.8.11",
    "@types/node": "~12.0.12",
    "@types/pubsub-js": "^1.8.1",
    "@types/react": "~17.0.0",
    "@types/react-dom": "~17.0.0",
    "@types/react-router": "~5.1.8",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-import": "^1.13.0",
    "bignumber.js": "^9.0.1",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "colors": "^1.4.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^9.0.0",
    "electron-builder": "^22.6.1",
    "error-overlay-webpack-plugin": "^0.4.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "i18next": "^21.3.3",
    "json-schema": "^0.2.5",
    "less": "^3.11.1",
    "less-loader": "^6.1.0",
    "lint-staged": "^10.2.6",
    "mini-css-extract-plugin": "^0.9.0",
    "minimist": "^1.2.5",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss": "^8.1.1",
    "postcss-flexbugs-fixes": "^4.2.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.7.0",
    "precss": "^4.0.0",
    "prettier": "^2.0.5",
    "react-i18next": "^11.12.0",
    "react-refresh": "^0.8.3",
    "shelljs": "^0.8.4",
    "speed-measure-webpack-plugin": "^1.3.3",
    "style-loader": "^1.2.1",
    "style-resources-loader": "^1.4.1",
    "styled-jsx": "3.3.0",
    "thread-loader": "^2.1.3",
    "typescript": "~3.9.3",
    "url-loader": "^4.1.0",
    "webpack": "~4.43.0",
    "webpack-cli": "~3.3.11",
    "webpack-dev-server": "~3.11.0",
    "webpackbar": "~4.0.0",
    "yamljs": "^0.3.0"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": ""
      }
    ],
    "asar": true,
    "asarUnpack": [
      "public",
      "node_modules/@spk"
    ],
    "appId": "com.electron-admin",
    "productName": "海阔天空",
    "files": [
      "dist",
      "public",
      "index.js",
      "config.js",
      "!**/*.{lib,obj,pdb,cc,h,tlog,map,exp,gypi,vcxproj,filters,cpp,c,ts}"
    ],
    "nodeGypRebuild": false,
    "extraFiles": [
      "server"
    ],
    "win": {
      "requestedExecutionLevel": "asInvoker",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "zip"
        }
      ],
      "icon": "public/assets/favicon/png/favicon_ch@5x.png"
    },
    "mac": {
      "icon": "public/assets/favicon/icons/favicon@5x.icns"
    },
    "linux": {
      "icon": "public/assets/favicon/icons/favicon@5x.icns",
      "category": "AudioVideo",
      "target": "deb",
      "executableName": "海阔天空"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "perMachine": true,
      "installerLanguages": "zh-CN"
    },
    "directories": {
      "output": "output"
    }
  },
  "window": {
    "center": true,
    "title": "Electron",
    "show": false,
    "width": 1440,
    "height": 800,
    "minWidth": 848,
    "minHeight": 480,
    "frame": false,
    "backgroundColor": "#fff",
    "webPreferences": {
      "webSecurity": false,
      "enableRemoteModule": true,
      "nodeIntegration": true
    }
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(ts|tsx)": [
      "eslint --color --fix --ext .ts,.tsx",
      "npm run lint:staged:tslint"
    ]
  }
}
